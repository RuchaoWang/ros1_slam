<?xml version="1.0" ?>
<sdf version="1.6">
  <world name="default">

    <!-- 物理参数：抑制下陷/穿透/爆炸 -->
    <physics type="ode">
      <max_step_size>0.001</max_step_size>
      <real_time_update_rate>1000</real_time_update_rate>
      <iters>200</iters>
    </physics>

    <!-- 常用地面/光照（可留可删） -->
    <include>
      <uri>model://ground_plane</uri>
    </include>
    <include>
      <uri>model://sun</uri>
    </include>

  </world>
</sdf>
